{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
    "max-len": ["error", 300],
    "properties": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
  }
}
